<odoo>
    <data noupdate="1">
        <record id="seq_draft_number" model="ir.sequence">
            <field name="name">Draft Number</field>
            <field name="code">draft.number</field>
            <field name="prefix">Draft.%(y)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
    	</record>

    	<record id="seq_bill_number" model="ir.sequence">
            <field name="name">Bill Number</field>
            <field name="code">bill.number</field>
            <field name="prefix">Draft.%(y)s-</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
    	</record>
    </data>
</odoo>
