<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for fal.project.budget -->
        <record id="seq_fal_project_budget" model="ir.sequence">
            <field name="name">Control</field>
            <field name="code">fal.project.budget</field>
            <field name="prefix">PJB</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>

        <record id="seq_fal_project_budget_line" model="ir.sequence">
            <field name="name">Control Item(s)</field>
            <field name="code">fal.project.budget.line</field>
            <field name="prefix">PBL</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>
