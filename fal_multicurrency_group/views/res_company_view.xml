<odoo>
    <data>
        <record id="fal_view_group_company_currency_form" model="ir.ui.view">
            <field name="name">fal_multi_currency_group.view_group_currency</field>
            <field name="model">res.company</field>
            <field name="inherit_id" ref="base.view_company_form"/>
            <field name="arch" type="xml">
                <field name="parent_id" position="after">
                    <field name="group_currency_id" groups="base.group_multi_company"/>
                </field>
                <field name="company_registry" position="after">
                    <field name="code" />
                </field>
            </field>
        </record>

    </data>
</odoo>
