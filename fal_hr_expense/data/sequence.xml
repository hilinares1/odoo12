<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="0">
        <record id="seq_expense_sheet" model="ir.sequence">
            <field name="name">Expense Sheet</field>
            <field name="code">expense.sheet</field>
            <field name="prefix">EXP.%(y)s-</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <data>
        <record forcecreate="True" id="decimal_expense" model="decimal.precision">
            <field name="name">Decimal Expense</field>
            <field name="digits">10</field>
        </record>
    </data>
</odoo>
